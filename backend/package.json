{
  "name": "shipsec-backend",
  "version": "0.1.0",
  "type": "module",
  "packageManager": "bun@1.1.20",
  "scripts": {
    "dev": "bun --watch src/main.ts",
    "start": "bun run src/main.ts",
    "build": "bun build src/main.ts --outdir build --target bun",
    "test": "bun test",
    "typecheck": "tsc --noEmit",
    "generate:openapi": "bun scripts/generate-openapi.ts",
    "worker:dev": "tsx src/temporal/workers/dev.worker.ts"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.0",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.4.0",
    "@nestjs/platform-express": "^10.4.0",
    "@nestjs/swagger": "^11.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "@temporalio/client": "^1.11.3",
    "@temporalio/worker": "^1.11.3",
    "@temporalio/workflow": "^1.11.3",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.6",
    "nestjs-zod": "^5.0.1",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "swagger-ui-express": "^5.0.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@nestjs/testing": "^10.4.0",
    "@types/node": "^20.16.11",
    "@types/pg": "^8.15.5",
    "bun-types": "^1.2.23",
    "drizzle-kit": "^0.31.5",
    "typescript": "^5.6.3"
  }
}
